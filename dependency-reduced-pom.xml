<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>fr.univ-nantes</groupId>
  <artifactId>ec-clem</artifactId>
  <version>1.0-SNAPSHOT</version>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.8.0</version>
        <configuration>
          <source>8</source>
          <target>8</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-install-plugin</artifactId>
        <version>3.0.0-M1</version>
        <executions>
          <execution>
            <id>install-icy</id>
            <phase>clean</phase>
            <goals>
              <goal>install-file</goal>
            </goals>
            <configuration>
              <groupId>fr.univ-nantes</groupId>
              <artifactId>icy</artifactId>
              <version>0.0.0</version>
              <file>${project.basedir}/libs/icy/icy.jar</file>
              <packaging>jar</packaging>
              <generatePom>true</generatePom>
            </configuration>
          </execution>
          <execution>
            <id>install-ezplug</id>
            <phase>clean</phase>
            <goals>
              <goal>install-file</goal>
            </goals>
            <configuration>
              <groupId>fr.univ-nantes</groupId>
              <artifactId>ezplug</artifactId>
              <version>0.0.0</version>
              <file>${project.basedir}/libs/icy/plugins/adufour/ezplug/EzPlug.jar</file>
              <packaging>jar</packaging>
              <generatePom>true</generatePom>
            </configuration>
          </execution>
          <execution>
            <id>install-vtk</id>
            <phase>clean</phase>
            <goals>
              <goal>install-file</goal>
            </goals>
            <configuration>
              <groupId>fr.univ-nantes</groupId>
              <artifactId>vtk</artifactId>
              <version>0.0.0</version>
              <file>${project.basedir}/libs/icy/lib/vtk.jar</file>
              <packaging>jar</packaging>
              <generatePom>true</generatePom>
            </configuration>
          </execution>
          <execution>
            <id>install-colorbar</id>
            <phase>clean</phase>
            <goals>
              <goal>install-file</goal>
            </goals>
            <configuration>
              <groupId>fr.univ-nantes</groupId>
              <artifactId>colorbar</artifactId>
              <version>0.0.0</version>
              <file>${project.basedir}/libs/icy/plugins/stef/tools/overlay/ColorBar.jar</file>
              <packaging>jar</packaging>
              <generatePom>true</generatePom>
            </configuration>
          </execution>
          <execution>
            <id>install-blocks</id>
            <phase>clean</phase>
            <goals>
              <goal>install-file</goal>
            </goals>
            <configuration>
              <groupId>fr.univ-nantes</groupId>
              <artifactId>blocks</artifactId>
              <version>0.0.0</version>
              <file>${project.basedir}/libs/icy/plugins/adufour/blocks/Blocks.jar</file>
              <packaging>jar</packaging>
              <generatePom>true</generatePom>
            </configuration>
          </execution>
          <execution>
            <id>install-tvdenoising</id>
            <phase>clean</phase>
            <goals>
              <goal>install-file</goal>
            </goals>
            <configuration>
              <groupId>fr.univ-nantes</groupId>
              <artifactId>tvdenoising</artifactId>
              <version>0.0.0</version>
              <file>${project.basedir}/libs/icy/plugins/nchenouard/tvdenoising/TVDenoising.jar</file>
              <packaging>jar</packaging>
              <generatePom>true</generatePom>
            </configuration>
          </execution>
          <execution>
            <id>install-filtertoolbox</id>
            <phase>clean</phase>
            <goals>
              <goal>install-file</goal>
            </goals>
            <configuration>
              <groupId>fr.univ-nantes</groupId>
              <artifactId>filtertoolbox</artifactId>
              <version>0.0.0</version>
              <file>${project.basedir}/libs/icy/plugins/adufour/filtering/FilterToolbox.jar</file>
              <packaging>jar</packaging>
              <generatePom>true</generatePom>
            </configuration>
          </execution>
          <execution>
            <id>install-bioformats</id>
            <phase>clean</phase>
            <goals>
              <goal>install-file</goal>
            </goals>
            <configuration>
              <groupId>fr.univ-nantes</groupId>
              <artifactId>bioformats</artifactId>
              <version>0.0.0</version>
              <file>${project.basedir}/libs/icy/lib/bioformats.jar</file>
              <packaging>jar</packaging>
              <generatePom>true</generatePom>
            </configuration>
          </execution>
          <execution>
            <id>install-jogl</id>
            <phase>clean</phase>
            <goals>
              <goal>install-file</goal>
            </goals>
            <configuration>
              <groupId>fr.univ-nantes</groupId>
              <artifactId>jogl</artifactId>
              <version>0.0.0</version>
              <file>${project.basedir}/libs/icy/lib/jogl-all.jar</file>
              <packaging>jar</packaging>
              <generatePom>true</generatePom>
            </configuration>
          </execution>
          <execution>
            <id>install-jai</id>
            <phase>clean</phase>
            <goals>
              <goal>install-file</goal>
            </goals>
            <configuration>
              <groupId>fr.univ-nantes</groupId>
              <artifactId>jai</artifactId>
              <version>0.0.0</version>
              <file>${project.basedir}/libs/icy/lib/jai_core.jar</file>
              <packaging>jar</packaging>
              <generatePom>true</generatePom>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-surefire-plugin</artifactId>
        <version>3.0.0-M3</version>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.2.1</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>org.junit.jupiter</groupId>
      <artifactId>junit-jupiter-engine</artifactId>
      <version>5.3.1</version>
      <scope>test</scope>
      <exclusions>
        <exclusion>
          <artifactId>apiguardian-api</artifactId>
          <groupId>org.apiguardian</groupId>
        </exclusion>
        <exclusion>
          <artifactId>junit-platform-engine</artifactId>
          <groupId>org.junit.platform</groupId>
        </exclusion>
        <exclusion>
          <artifactId>junit-jupiter-api</artifactId>
          <groupId>org.junit.jupiter</groupId>
        </exclusion>
      </exclusions>
    </dependency>
  </dependencies>
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
</project>
